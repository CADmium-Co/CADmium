{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", "dist/**", "pkg/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:unit": {},
    "test:e2e": {},
    "lint": {},
    "format": {
      "cache": false,
      "persistent": false
    },
    "preview": {},
    "clean": {
      "cache": false,
      "persistent": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
